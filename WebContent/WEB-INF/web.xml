<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee         
        http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
        
        
   <servlet>
       <servlet-name>Kies Bakje</servlet-name>
       <servlet-class>web.BeerSelect</servlet-class>
   </servlet>

   <servlet-mapping>
       <servlet-name>Kies Bakje</servlet-name>
       <url-pattern>/Bakje.do</url-pattern>
   </servlet-mapping>
   
   
   
   <servlet>
       <servlet-name>Start Spel</servlet-name>
       <servlet-class>web.StartGame</servlet-class>
   </servlet>

   <servlet-mapping>
       <servlet-name>Start Spel</servlet-name>
       <url-pattern>/StartGame.do</url-pattern>
   </servlet-mapping>
</web-app>